#include <iostream>
using namespace std;

int main() {
    // insert code here...
    int a, b, c, d;
    
    b = 0;
    c = 0;
    
    int par[5];
    int impar[5];
    
    for(int x = 0; x < 15; x++){
        scanf("%d", &a);
        if(a%2 == 0){
            par[b] = a;
            b++;
        }else{
            impar[c] = a;
            c++;
        }
        if (b == 5) {
            d = 0;
            while (d != 5) {
                cout<<"par[" << d << "] = " << par[d] << "\n";
                d++;
            }
            b = 0;
        }
        if (c == 5) {
            d = 0;
            while (d != 5) {
                cout << "impar[" << d << "] = "<< impar[d] << "\n";
                d++;
            }
            c = 0;
        }
        if (x == 14) {
            d = 0;
            while (d < c) {
                cout << "impar[" << d << "] = " << impar[d] << "\n";
                d++;
            }
            d = 0;
            while (d < b) {
                cout << "par[" << d << "] = " << par[d] << "\n";
                d++;
            }
        }
    }
    return 0;
}
